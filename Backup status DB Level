COL OUTPUT_BYTES_DISPLAY FOR A20
alter session set nls_date_format='DD-MON-YYYY HH24:MI:SS';
SELECT NAME,OPEN_MODE FROM V$DATABASE;
SELECT START_TIME,END_TIME,STATUS,AUTOBACKUP_DONE,OUTPUT_BYTES_DISPLAY FROM  V$RMAN_BACKUP_SUBJOB_DETAILS 
WHERE END_TIME > (SELECT SYSDATE-1 FROM DUAL) ORDER BY END_TIME;
